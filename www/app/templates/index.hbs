<div class="panel panel-success" style="width: 75%; margin: auto;">
  <!-- Default panel contents -->
  <div class="panel-heading">
    <h3 class="panel-title">Pool Stats</h3>
  </div>
  <!-- Table -->
  <table class="table" style="width: 100%;">
    <h5> Pool Settings </h5>
    <tr>
      <th class="tg-0lax">Setting</th>
      <th class="tg-0lax">Value</th>
    </tr>
    <tr>
      <td class="tg-0lax">Minimum Payout Amount:</td>
      <td class="tg-0lax">{{config.PayoutThreshold}}</td>
    </tr>
    <tr>
      <td class="tg-0lax">Pool Payment Method:</td>
      <td class="tg-0lax">PROP</td>
    </tr>
    <tr>
      <td class="tg-0lax">Payout Frequency:</td>
      <td class="tg-0lax">60 Minutes</td>
    </tr>
    <h5> Pool Stats </h5>
    <tr>
      <th class="tg-0lax">Stat</th>
      <th class="tg-0lax">Value</th>
    </tr>
  </table>
</div>
